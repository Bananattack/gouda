    mapper = 1,
    mirroring = 1

bank ram : ram[0xFFFF]
bank prg : prg[0x4000]
bank chr : chr[0x2000]

in ram, 0x00:
    var t0, t1, t2, t3, t4, t5, t6, t7, t8, t9, t10, t11, t12, t13, t14, t15: byte
    
    var camera_x_lo, camera_x_hi : byte
    var camera_y_lo, camera_y_hi : byte
    
    var world_ptr_lo, world_ptr_hi : byte
    var draw_request : byte
    
    enum:
        DIRECTION_RIGHT,
        DIRECTION_LEFT
    
    var player_x_lo, player_x_hi : byte
    var player_y_lo, player_y_hi : byte
    
    let __MAP_WIDTH = 2
    var map_width : byte
    
    var unpress : byte
    var controls : byte
    
    var sprite_index : byte
    var render_flags : byte
    
in ram, 0x200:
    var sprite_buffer : byte[256]
    
in ram, 0x300:
    // ...
    
in ram, 0x6000:
    package vram_loader
        var code_buffer : byte[1446] // 1440 bytes + 5 bytes of page-ending NOP + 1 byte return
        var code_pointer : word      // Copy back when loop ends.
    end
    
in prg, 0xC000:
    include 'hardware.nel'
    include 'tilemap.nel'
    include 'sprite.nel'
    include 'player.nel'
    include 'camera.nel'

    def load_palette: begin
        a: get #>ppu.PALETTE_BASE, put @ppu.address, get #<ppu.PALETTE_BASE, put @ppu.address
        x: get #0
        repeat
            a: get @palette[x], put @ppu.data
            x: inc, cmp #(ppu.PALETTE_SIZE * ppu.PALETTE_TOTAL)
        until zero
        return
    end
    
    // Reads controller and stores the result in controls
    // Derived from code posted by blargg on nesdevwiki
    def check_controls: begin
        // Strobe controller
        a: get #1, put @joy.out, get #0, put @joy.out
        // Read all 8 buttons
        x: get #8
        repeat
            // Read next button state and mask off low 2 bits.
            // Compare with $01, which will set carry flag if
            // either or both bits are set.
            a: get @joy.in1, and #0x3, cmp #1
            // Now, rotate the carry flag into the top of A,
            // land shift all the other buttons to the right
            @controls: ror
            x: dec
        until zero
        // Remove unpress flag for controls no longer being held.
        a: get @unpress, and @controls, put @unpress
        // Remove controls that have the unpress flag set.
        // (Keep controls that don't have the unpress flag set)
        a: get @unpress, xor #0xFF, and @controls, put @controls
        return
    end
    
    def print_text: begin
        let START_X : byte = 10
        let START_Y : byte = 13
        let TILE_ADDRESS : word = ppu.NAMETABLE_BASE + (START_Y * 32 + START_X)
        
        // Read PPU status to reset its state.
        a: get @ppu.status
        // Now setup the PPU for copying tiles.
        a: get #>TILE_ADDRESS, put @ppu.address, get #<TILE_ADDRESS, put @ppu.address
        
        x: get #0
        a: get @message[x]
        while not zero do
            x: inc
            a: put @ppu.data
            a: get @message[x]
        end
        return
    end

    def main: begin
        // Set necessary processor flags.
        p: unset decimal, set interrupt
        // Prepare stack.
        x: get #0xFF, put s
        
        // Turn off rendering.
        x: get #0, put @ppu.ctrl, put @ppu.mask
        
        // Reset MMC1 shift register
        a: get #0x80, put @0x8000
        // Setup MMC1 bank switching and mirroring settings.
        a: get #0b11110, put @0x8000, shr, put @0x8000, shr, put @0x8000, shr, put @0x8000, shr, put @0x8000 
        // Setup CHR banks.
        a: get #0x00, put @0xA000, shr, put @0xA000, shr, put @0xA000, shr, put @0xA000, shr, put @0xA000
        a: get #0x01, put @0xC000, shr, put @0xC000, shr, put @0xC000, shr, put @0xC000, shr, put @0xC000
        
        // Wait for the PPU to be ready to use, which takes 2 vertical blanks.
        repeat
            repeat
                a: bit @ppu.status
            until negative
            x: inc, cmp #2
        until =
        
        x: get #0
        repeat
            a: get #0x00, put @0x000[x], put @0x400[x]
            a: get #0xFE, put @0x200[x]
            a: get #2, put @0x300[x]
            x: inc
        until zero
        
        call load_palette

        // Setup the world map pointer.
        a: get #<world_map, put @world_ptr_lo, get #>world_map, put @world_ptr_hi
        // Init the tileset pointer.
        a: get #<tile_top_left, put @t0, get #>tile_top_left, put @t1
        // Setup the map pointer for the first screen
        a: get @world_ptr_lo, put @t2, get @world_ptr_hi, put @t3
        // Nametable 0.
        a: get #0, put @t4
        call tilemap.load
        
        // We're finally ready to show the screen!
        a: get #(ppu.CTRL_NMI | ppu.CTRL_SPRITE_PATTERN_1), put @ppu.ctrl, put @render_flags
        a: get #(ppu.MASK_LEFTMOST_BG | ppu.MASK_LEFTMOST_SPRITES | ppu.MASK_RENDER_BG | ppu.MASK_RENDER_SPRITES), put @ppu.mask

        a: get #apu.FLAG_SQUARE1, put @apu.flag
        
        a: get #__MAP_WIDTH, put @map_width
        
        a: get #0x68, put @player_y_lo
        
        repeat
            // Initialize code pointer for video RAM loading.
            a: get #<vram_loader.code_buffer, put @vram_loader.code_pointer
            a: get #>vram_loader.code_buffer, put @(vram_loader.code_pointer + 1)
            
            call check_controls
            call player.update
            call camera.follow_player
            
            // Write a `return` to the code buffer.
            y: get #0
            a: get @vram_loader.code_pointer, put @t0
            a: get @(vram_loader.code_pointer + 1), put @t1
            a: get #0x60, put @[t0][y]
            
            // Reset the sprite index.
            a: get #0, put @sprite_index
            
            // Draw sprites.
            call player.draw
            
            // Request redraw.
            @draw_request: inc
            // Busy wait for completion.
            repeat
                a: get @draw_request
            until zero
        end
    end
    
    def draw: begin
        // Save registers.
        a: push, get x, push, get y, push
        
        // Should we draw things?
        a: get @draw_request
        if not zero then
            // Jump into self-modifying code.
            call vram_loader.code_buffer
            
            // Render sprites.
            a: get #<sprite_buffer, put @ppu.oam.address, get #>sprite_buffer, put @ppu.oam.dma
            
            // We're done drawing.
            @draw_request: dec
        end
        
        // Update scroll.
        a: get @camera_x_hi, and #1, or @render_flags, put @ppu.ctrl
        a: get @camera_x_lo, put @ppu.scroll, get #0, put @ppu.scroll
        
        // Acknowledge vblank.
        a: get @ppu.status
        
        // Restore registers.
        a: pull, put y, pull, put x, pull
        resume
    end
    
    def scan: begin
        a: push, get x, push, get y, push
        a: pull, put y, pull, put x, pull
        resume        
    end

