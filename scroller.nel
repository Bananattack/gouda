    def message:
        // A null-terminated message.
        byte: "HELLO  WORLD", 0
    
    def palette:
        let BACKGROUND_COLOR = 0x0F
        
        // Tiles
        byte: BACKGROUND_COLOR, 0x23, 0x17, 0x38
        byte: BACKGROUND_COLOR, 0x13, 0x17, 0x38
        byte: BACKGROUND_COLOR, 0x03, 0x1C, 0x29
        byte: BACKGROUND_COLOR, 0x13, 0x23, 0x10
        // Sprites
        byte: BACKGROUND_COLOR, 0x08, 0x26, 0x37
        byte: BACKGROUND_COLOR, 0x00, 0x28, 0x30
        byte: BACKGROUND_COLOR, 0x0F, 0x0F, 0x0F
        byte: BACKGROUND_COLOR, 0x0F, 0x0F, 0x0F
    
